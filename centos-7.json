{
    "builders": [{
        "boot_command": "<tab> ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter>",
        "boot_wait": "5s",
        "disk_size": "20480",
        "guest_os_type": "RedHat_64",
        "hard_drive_interface": "sata",
        "headless": true,
        "http_directory": "kickstart",
        "iso_checksum": "f90e4d28fa377669b2db16cbcb451fcb9a89d2460e3645993e30e137ac37d284",
        "iso_checksum_type": "sha256",
        "iso_url": "http://ftp.vectranet.pl/centos/7/isos/x86_64/CentOS-7-x86_64-Minimal-1511.iso",
        "shutdown_command": "echo 'vagrant' | sudo -S /usr/sbin/poweroff",
        "ssh_password": "vagrant",
        "ssh_pty": true,
        "ssh_timeout": "30m",
        "ssh_username": "vagrant",
        "type": "virtualbox-iso",
        "vboxmanage": [
            [
                "modifyvm",
                "{{.Name}}",
                "--memory",
                "1024"
            ]
        ],
        "vm_name": "CentOS 7"
    }],
    "post-processors": [{
        "compression_level": 9,
        "only": [
            "virtualbox-iso"
        ],
        "output": "builds/{{.Provider}}-centos-7.box",
        "type": "vagrant"
    }]
}
